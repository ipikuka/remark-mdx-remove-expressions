{
  "name": "remark-mdx-remove-expressions",
  "version": "1.0.3",
  "description": "Remark plugin to remove MDX expressions within curlybraces {} in MDX content",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "rimraf dist && tsc --build && type-coverage",
    "format": "npm run prettier && npm run lint",
    "prettier": "prettier --write .",
    "lint": "eslint .",
    "test": "vitest --watch=false",
    "test:watch": "vitest",
    "test:file": "vitest test.spec.ts",
    "prepack": "npm run build",
    "prepublishOnly": "npm run test && npm run format && npm run test-coverage",
    "test-coverage": "vitest run --coverage"
  },
  "files": [
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipikuka/remark-mdx-remove-expressions.git"
  },
  "keywords": [
    "unified",
    "mdast",
    "markdown",
    "MDX",
    "remark",
    "plugin",
    "remark plugin",
    "mdxjsEsm",
    "mdx remove esm",
    "mdx remove mdxjsEsm"
  ],
  "author": "ipikuka <talatkuyuk@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/ipikuka/remark-mdx-remove-expressions#readme",
  "bugs": {
    "url": "https://github.com/ipikuka/remark-mdx-remove-expressions/issues"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@mdx-js/mdx": "^3.1.1",
    "@types/estree": "^1.0.8",
    "@types/node": "^24.10.13",
    "@types/react-dom": "^19.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/eslint-plugin": "^1.6.9",
    "dedent": "^1.7.1",
    "eslint": "^10.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.3.0",
    "mdast-util-mdx-expression": "^2.0.1",
    "mdast-util-mdx-jsx": "^3.2.0",
    "next-mdx-remote": "^6.0.0",
    "next-mdx-remote-client": "^2.1.9",
    "prettier": "^3.8.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "remark-gfm": "^4.0.1",
    "remark-mdx": "^3.1.1",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "rimraf": "^6.1.3",
    "type-coverage": "^2.29.7",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "unified": "^11.0.5",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@types/mdast": "^4.0.4",
    "unist-util-remove": "^4.0.0"
  },
  "peerDependencies": {
    "unified": "^11"
  },
  "typeCoverage": {
    "atLeast": 100,
    "detail": true,
    "ignoreAsAssertion": true,
    "ignoreCatch": true,
    "strict": true
  },
  "sideEffects": false
}
